{  
   "name":"dma/dma_simple_grid",
   "description":"Contao Extension for simple multiple grid-support",
   "keywords":[  
      "contao",
      "extension",
      "module",
      "grid"
   ],
   "type":"contao-module",
   "homepage":"http://www.dma.do",
   "license":"LGPL-3.0+",
   "authors":[
      {
         "name":"DMA GmbH",
         "homepage":"http://www.dma.do",
         "role": "origin maintainer"
      },
      {  
         "name":"Janosch Oltmanns",
         "email":"kontakt@janosch-oltamnns.de",
         "homepage":"https://www.janosch-oltmanns.de",
         "role": "maintainer"
      }
   ],
   "support":{  
      "issues":"https://github.com/DMAGmbH/dma_simple_grid/issues",
      "source":"https://github.com/DMAGmbH/dma_simple_grid"
   },
   "require":{
      "php": "^7.2 || ^8.0",
      "contao/core-bundle":"^4.9",
      "menatwork/contao-multicolumnwizard-bundle": "^3.5"
   },
   "autoload": {
      "psr-4": {
         "Dma\\SimpleGrid\\": "src"
      },
      "classmap": [
         "src/Resources/contao/"
      ],
      "exclude-from-classmap": [
         "src/Resources/contao/config/",
         "src/Resources/contao/dca/",
         "src/Resources/contao/languages/",
         "src/Resources/contao/templates/"
      ]
   },
   "extra":{
      "contao-manager-plugin": "Dma\\SimpleGrid\\ContaoManager\\Plugin"
   }
}